{
  "name": "clutch",
  "version": 2,
  "builds": [
    { "src": "client/package.json", "use": "@vercel/static-build" },
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/game", "dest": "api/index.js" },
    { "src": "^/favicon.ico", "dest": "/client/favicon.ico" },
    { "src": "^/(css|js)/(.+)", "dest": "/client/$1/$2" },
    { "src": "^/.*", "dest": "/client/index.html" }
  ]
}
